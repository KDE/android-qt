//! [4]
// forward                                  // backward
QMutableListIterator<int> i(list);          QMutableListIterator<int> i(list);  
while (i.hasNext())                         i.toBack();                         
    if (i.next() > 128)                     while (i.hasPrev())                 
        i.setValue(128);                        if (i.prev() > 128)             
                                                    i.setValue(128);           
//! [4]


