// forward                                  // backward
QMutableListIterator<int> i(list);          QMutableListIterator<int> i(list);                 
while (i.hasNext())                         i.toBack();                         
    if (i.next() % 2 != 0)                  while (i.hasPrev())                        
        i.remove();                             if (i.prev() % 2 != 0)          
                                                    i.remove();                 
