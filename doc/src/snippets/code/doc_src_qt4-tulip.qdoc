// forward                                  // backward
QList<QString> list;                        QList<QString> list;
...                                         ...
QListIterator<QString> i(list);             QListIterator<QString> i(list);        
while (i.hasNext())                         i.toBack();                            
    cout << i.next().ascii() << endl;       while (i.hasPrev())                    
                                                cout << i.prev().ascii() << endl;
