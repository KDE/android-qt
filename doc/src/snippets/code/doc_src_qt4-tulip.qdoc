//! [7]
// STL-style                                // Java-style
QList<int>::iterator i = list.begin();      QMutableListIterator<int> i(list);
while (i != list.end()) {                   while (i.hasNext()) {
    if (*i == 0) {                              int val = i.next();
        i = list.erase(i);                      if (val < 0)
    } else {                                        i.setValue(-val);
        if (*i < 0)                             else if (val == 0)
            *i = -*i;                               i.remove();
        ++i;                                }
    }
}
