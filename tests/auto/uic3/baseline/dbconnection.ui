            <y>0</y>
            <width>199</width>
            <height>185</height>
        </rect>
    </property>
    <property name="caption">
        <string>Edit Database Connection</string>
    </property>
    <grid>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <property name="margin">
            <number>0</number>
