      <item row="1" column="0">
        <widget class="Q3ButtonGroup" name="ButtonGroup1">
          <property name="title">
            <string>Pixmaps</string>
          </property>
          <layout class="QGridLayout">
            <property name="margin">
              <number>11</number>
            </property>
            <property name="spacing">
              <number>6</number>
