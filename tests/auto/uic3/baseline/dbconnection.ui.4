            <bool>false</bool>
          </property>
        </widget>
      </item>
      <item row="5" column="1">
        <widget class="QLineEdit" name="editHostname">
          <property name="enabled">
            <bool>false</bool>
          </property>
        </widget>
      </item>
      <item row="6" column="0">
