            <cstring>comboDriver</cstring>
          </property>
          <property name="wordWrap">
            <bool>false</bool>
          </property>
        </widget>
      </item>
      <item row="0" column="1">
        <widget class="QLineEdit" name="editName">
          <property name="enabled">
            <bool>false</bool>
          </property>
        </widget>
      </item>
      <item row="5" column="0">
        <widget class="QLabel" name="TextLabel5">
          <property name="text">
            <string>&amp;Hostname:</string>
          </property>
