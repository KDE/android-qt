            <cstring>editHostname</cstring>
          </property>
          <property name="wordWrap">
            <bool>false</bool>
          </property>
        </widget>
      </item>
      <item row="1" column="1">
        <widget class="QComboBox" name="comboDriver">
          <property name="enabled">
            <bool>false</bool>
          </property>
          <property name="editable">
            <bool>true</bool>
          </property>
        </widget>
      </item>
      <item row="0" column="0">
        <widget class="QLabel" name="TextLabel1">
          <property name="text">
            <string>&amp;Name:</string>
          </property>
